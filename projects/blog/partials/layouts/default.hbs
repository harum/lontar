<!DOCTYPE html>
<html dir="ltr" lang="en">
  <head>
    {{> includes/meta }}
    {{> includes/google-analytics }}
    <link rel="stylesheet" media="all" href="/assets/css/main.css">
    <script type='module' src="/assets/js/modules/index.js"></script>
  </head>

  <body class="o-container o-container--medium o-page u-pad-h--xlarge">
    {{> includes/header }}

    <main
      role="main"
      class="page__content"
    >
      <h1>{{ title }}</h1>
      {{#> content-block}}
      {{/content-block}}
    </main>

    {{> includes/update-content }}
    {{> includes/footer }}
    {{> includes/back-to-top }}

    <script nomodule src="/assets/js/bundles/index.js"></script>
  </body>
</html>
